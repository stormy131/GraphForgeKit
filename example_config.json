{
    "task": {
        "type": "regression",
        "target_idx": 0
    },

    "gnn_config": {
        "encoder_schema":   [256, 256],
        "estimator_schema": [128, 128, 7],
        "convolution":      "GCN"
    },

    "edges": [
        {
            "type": "Distance",
            "spatial": {
                "idx": null,
                "exclude": false
            },

            "kwargs": {
                "max_dist": 1,
                "cache_id": "cora_dist",
                "cache_dir": "./data/cache/edges"
            }
        },

        {
            "type": "ClustRepr",
            "spatial": {
                "idx": null,
                "exclude": false
            },

            "kwargs": {
                "neighbor_rate": 0.7,
                "cache_id": "cora_repr",
                "cache_dir": "./data/cache/edges"
            }
        }

    ]
}
